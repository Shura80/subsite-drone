<?php

/**
 * @file
 * Install file for the agri_usermerge feature.
 */

/**
 * Implements hook_enable().
 */
function agri_usermerge_enable() {
  drupal_set_message(t('Agri Usermerge feature is now active on your site.'));
}

/**
 * Implements hook_disable().
 */
function agri_usermerge_disable() {
  drupal_set_message(t('Agri Usermerge feature is now inactive on your site.'));
}
