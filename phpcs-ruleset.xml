<?xml version="1.0"?>

<ruleset name="Subsite">
    <description>Custom ruleset for subsite usage.</description>

    <!-- EntityDefaultUIController hooks do not follow Drupal function name conventions.-->
    <rule ref="Drupal.NamingConventions.ValidFunctionName.ScopeNotCamelCaps">
        <exclude-pattern>**/Views/EnrdSfrViewsController.php</exclude-pattern>
        <exclude-pattern>**/EnrdSfrTypeUIController.php</exclude-pattern>
    </rule>
</ruleset>